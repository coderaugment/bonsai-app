{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "lsof -ti:3080 | xargs kill -9 2>/dev/null; next dev --hostname 0.0.0.0 --port 3080",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:migrate:dev": "BONSAI_ENV=dev tsx src/db/migrate.ts",
    "db:seed": "BONSAI_ENV=dev tsx src/db/seed.ts",
    "db:seed:prod": "tsx src/db/seed.ts",
    "db:reset": "BONSAI_ENV=dev tsx src/db/reset-clean.ts",
    "db:reset-test": "BONSAI_ENV=dev tsx src/db/reset-test.ts",
    "db:studio": "drizzle-kit studio",
    "prompts:sync": "BONSAI_ENV=dev tsx scripts/sync-role-prompts.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.73.0",
    "age-encryption": "^0.3.0",
    "better-sqlite3": "^12.6.2",
    "drizzle-orm": "^0.45.1",
    "html2canvas": "^1.4.1",
    "next": "16.1.6",
    "pngjs": "^7.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.0.0",
    "@vitest/ui": "^3.0.0",
    "babel-plugin-react-compiler": "1.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^3.0.0"
  }
}
